{
  "schemaName": "gw.content.cc.admin.v1.admin_content-1.0",
  "combine": [
    "gw.content.cc.common.v1.common_content-1.0",
    "gw.core.cc.admin.v1.admin_core-1.0"
  ],
  "mappers": {
    "User": {
      "root": "entity.User",
      "properties": {
        "firstNameKanji": {
          "path": "(User.Contact as Person).FirstNameKanji",
          "predicate": "User.Contact typeis Person"
        },
        "lastNameKanji": {
          "path": "(User.Contact as Person).LastNameKanji",
          "predicate": "User.Contact typeis Person"
        },
        "particle": {
          "path": "(User.Contact as Person).Particle",
          "predicate": "User.Contact typeis Person"
        }
      }
    }
  }
}