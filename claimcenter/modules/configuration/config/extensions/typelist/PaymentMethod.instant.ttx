<?xml version="1.0"?>
<typelistextension xmlns="http://guidewire.com/typelists" name="PaymentMethod">
  <typecode
    code="instant"
    desc="Instant payment"
    name="Instant"
    priority="4"/>
  <typefilter
    desc="The payment methods that applies to a person payee"
    name="personcheckpayee">
    <include
      code="check"/>
    <include
      code="eft"/>
    <include
      code="instant"/>
  </typefilter>
  <typefilter
    desc="The payment methods that applies to a payee that is not a person"
    name="nonpersoncheckpayee">
    <include
      code="check"/>
    <include
      code="eft"/>
  </typefilter>
</typelistextension>