<input type="text"
       name="${renderId}"
       class="${noTrack ? 'gw-noTrack'}"
       aria-labelledby="${inputLabelAriaId}"
       disabled="${disabled}"
       aria-required="${required ? 'true' : 'false'}"
       aria-invalid="${invalid ? 'true' : 'false'}"
       value="${currentValue}"
       maxlength="${maxlength}"
       readonly="${readonly}"
       placeholder="${placeholder}"/>
