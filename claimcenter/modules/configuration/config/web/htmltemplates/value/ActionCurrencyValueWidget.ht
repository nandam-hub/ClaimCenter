<div :wrapIf="altValue || action" data-gw-click="${action}" data-gw-no-tab-index="${isUnclickable}" aria-hidden="${isUnclickable}"
  role="button" aria-describedby="${renderId}--altValue"
  class="gw-CurrencyValue--modelValue ${action?'gw-ActionValueWidget'}">
  ${currentValue}
</div>
<div :if="altValue" id="${renderId}--altValue" class="gw-CurrencyValue--altValue">${altValue}</div>
