<?xml version="1.0"?>
<!-- ==============================================================================
   Messaging Configuration
  ============================================================================== -->
<messaging-config
  xmlns="http://guidewire.com/messaging-config"
  defaultServer="#messaging">
  <messaging>
	<destination
      id="11"
      name="SP.ActivityManagement.MessageDestination.Name"
      transportplugin="CrossAppActivityTransport_SP">
      <event
        name="SP_PC_ActPatternAdded"/>
      <event
        name="SP_PC_ActPatternUpdated"/>
      <event
        name="SP_PC_ActPatternRemoved"/>
      <event
        name="SP_BC_ActPatternAdded"/>
      <event
        name="SP_BC_ActPatternUpdated"/>
      <event
        name="SP_BC_ActPatternRemoved"/>
    </destination>
    <destination
      id="10"
      name="SP.DataAudit.Admin.Audit.MessageDestination.Name"
      numsenderthreads="5"
      transportplugin="DataAuditTransport_SP">
      <event
        name="SendToExternalSystem"/>
    </destination>
    <destination
      id="80"
      initialretryinterval="100"
      internal="true"
      name="Java.MessageDestination.ContactAutoSyncFailure.Name"
      transportplugin="autoSyncFailureTransport">
      <event
        name="ContactAutoSyncFailed"/>
    </destination>
    <destination
      id="67"
      internal="true"
      maxretries="5"
      name="Java.MessageDestination.Metro.Name"
      requestplugin="metroRequest"
      transportplugin="metroTransport">
      <event
        name="MetroReportAdded"/>
      <event
        name="MetroReportChanged"/>
    </destination>
    <destination
      disabled="true"
      id="111"
      initialretryinterval="100"
      internal="true"
      maxretries="20"
      name="Java.MessageDestination.JMSMessageTransport.Name"
      replyplugin="jmsReply"
      transportplugin="jmsTransport">
      <event
        name="NoteAdded"/>
    </destination>
    <destination
      disabled="true"
      id="68"
      initialretryinterval="100"
      internal="true"
      name="Java.MessageDestination.ConsoleMessageLogger.Name"
      transportplugin="consoleTransport">
      <event
        name="\w*"/>
    </destination>
    <destination
      id="65"
      initialretryinterval="500"
      internal="true"
      maxretries="5"
      name="Java.MessageDestination.Email.Name"
      transportplugin="emailMessageTransport"/>
    <destination
      disabled="true"
      id="66"
      initialretryinterval="500"
      internal="true"
      maxretries="5"
      name="Java.MessageDestination.ISO.Name"
      replyplugin="IISOReplyPlugin"
      transportplugin="isoTransport">
      <event
        name="ExposureAdded"/>
      <event
        name="ExposureValid"/>
      <event
        name="ExposureChanged"/>
      <event
        name="ClaimValid"/>
      <event
        name="ClaimChanged"/>
      <event
        name="ClaimResync"/>
      <event
        name="PolicyChanged"/>
      <event
        name="ClaimContactContactChanged"/>
      <event
        name="ClaimContactRoleAdded"/>
      <event
        name="ClaimContactRoleChanged"/>
      <event
        name="ClaimContactRoleRemoved"/>
    </destination>
    <destination
      disabled="true"
      id="69"
      internal="true"
      name="Java.MessageDestination.PolicySystemNotification.Name"
      transportplugin="policySystemNotificationTransport">
      <event
        name="ClaimExceedsLargeLoss"/>
      <event
        name="ClaimResync"/>
    </destination>
    <!-- Message destination 324 is referenced in the DocumentsActionsUIHelper to determine whether the documentStoreTransport
    is suspended or not. -->
    <destination
      disabled="false"
      id="324"
      internal="true"
      name="Java.MessageDestination.DocumentStore.Name"
      transportplugin="documentStoreTransport">
      <event
        name="DocumentStore"/>
    </destination>
    <destination
      id="99"
      internal="true"
      name="Java.MessageDestination.ContactMessageTransport.Name"
      primaryentity="Contact"
      requestplugin="ContactMessageRequest"
      transportplugin="ContactMessageTransport">
      <event
        name="ContactAdded"/>
      <event
        name="ContactChanged"/>
      <event
        name="ContactRemoved"/>
      <event
        name="ContactUpdatedInAB"/>
      <event
        name="ContactTagsUpdated"/>
      <event
        name="ClaimContactRemoved"/>
    </destination>
    <destination
      id="70"
      internal="true"
      name="Java.MessageDestination.SolrMessageTransport.ClaimContact.Name"
      transportplugin="CCClaimContactSolrMessageTransportPlugin">
      <event
        name="ClaimAdded"/>
      <event
        name="ClaimChanged"/>
      <event
        name="ClaimRemoved"/>
      <event
        name="ClaimPurged"/>
      <event
        name="ClaimContactAdded"/>
      <event
        name="ClaimContactContactChanged"/>
      <event
        name="ClaimContactRemoved"/>
      <event
        name="ClaimContactRoleAdded"/>
      <event
        name="ClaimContactRoleChanged"/>
      <event
        name="ClaimContactRoleRemoved"/>
      <event
        name="ClaimInfoChanged"/>
      <event
        name="ClaimInfoPurged"/>
    </destination>
    <destination
      id="75"
      internal="true"
      name="Java.MessageDestination.SolrMessageTransport.Claim.Name"
      transportplugin="CCClaimSolrMessageTransportPlugin">
      <event
        name="ClaimInfoPurged"/>
      <event
        name="ClaimInfoChanged"/>
    </destination>
    <destination
      disabled="${MESSAGING_APPEVENTS_DISABLED:true}"
      extendedprimaryobjectsearch="true"
      id="500"
      internal="true"
      name="Java.MessageDestination.EventBus"
      numsenderthreads="10"
      pollinterval="1000"
      singlethreadandblockonnonsafeorderedmessages="true"
      transportplugin="EventMessageTransport"
      useprimaryobjectidcolumn="true">
      <event
        name=".*"/>
    </destination>
    <destination
      disabled="${MESSAGING_APPEVENTS_DISABLED:true}"
      extendedprimaryobjectsearch="true"
      id="599"
      internal="true"
      name="Java.MessageDestination.EventBusAdmin"
      pollinterval="1000"
      singlethreadandblockonnonsafeorderedmessages="true"
      transportplugin="EventMessageTransport">
      <event
        name="AppEventsTest"/>
      <event
        name="AuthorityLimitProfile(Added|Changed|Removed)"/>
      <event
        name="Group(Added|Changed|Removed)"/>
      <event
        name="Organization(Added|Changed|Removed)"/>
      <event
        name="Role(Added|Changed|Removed)"/>
      <event
        name="User(Added|Changed|Removed)"/>
    </destination>
    <destination
      id="71"
      internal="true"
      name="Java.MessageDestination.InstantCheckTransport.Name"
      transportplugin="InstantCheckTransportPlugin">
      <event
        name="CheckAdded"/>
      <event
        name="CheckChanged"/>
    </destination>
  </messaging>
</messaging-config>
